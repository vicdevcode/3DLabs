                </mesh:Tetrahedron3D>

                <mesh:Octahedron3D x:Name="Octahedron1"
                Size="0.5" 
                Position="-1.5,-0.7,0" 
                Front="Red" >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
