<Window x:Class="labs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mesh="clr-namespace:labs.Meshes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Viewport3D>
            <Viewport3D.Camera>
                <!--Установка камеры - перспективная проекция-->
                <PerspectiveCamera Position="0,0,5" LookDirection="0,0,-2" />
            </Viewport3D.Camera>

            <Viewport3D.Children>
                <ModelVisual3D>
                    <ModelVisual3D.Content>
                        <!--Установка освещения - прямой свет-->
                        <DirectionalLight Color="White" Direction="-1,-1,-2" />
                    </ModelVisual3D.Content>
                </ModelVisual3D>

                <mesh:Cube3D x:Name="Cube1"
                Size="0.5" 
                Position="-1.5,0.7,0" 
                Front="red" 
                Top="blue" 
                Left="yellow" 
                Back="purple" 
                Bottom="green" 
                Right="orange" >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
                            <RotateTransform3D CenterX="-1.5" CenterY="0.7">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                                    Axis="1,0,0" 
                                    Angle="0" 
                                    x:Name="rotateX"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="-1.5" CenterY="0.7">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                                    Axis="0,1,0" 
                                    Angle="0" 
                                    x:Name="rotateY"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="-1.5" CenterY="0.7">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                                    Axis="0,0,1" 
                                    Angle="0" 
                                    x:Name="rotateZ"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                        </Transform3DGroup>
                    </ModelVisual3D.Transform>
                </mesh:Cube3D>

                <mesh:TruncatedPyramid x:Name="TruncatedPyramid1"
Position="1,-0.8,0" 
Front="red" 
Top="blue" 
Left="yellow" 
Back="purple" 
Bottom="green" 
Right="orange" >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
                            <RotateTransform3D CenterX="1" CenterY="-0.8">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="1,0,0" 
                    Angle="0" 
                    x:Name="rotateX8"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="1" CenterY="-0.8">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="0,1,0" 
                    Angle="0" 
                    x:Name="rotateY8"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="1" CenterY="-0.8">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="0,0,1" 
                    Angle="0" 
                    x:Name="rotateZ8"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                        </Transform3DGroup>
                    </ModelVisual3D.Transform>
                </mesh:TruncatedPyramid>

                <mesh:Cone x:Name="Cone1"
Position="0,0.6,0" Radius="0.26" Height="0.5" Slices="100"
 >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
                            <RotateTransform3D CenterY="0.6">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="1,0,0" 
                    Angle="0" 
                    x:Name="rotateX9"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterY="0.6">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="0,1,0" 
                    Angle="0" 
                    x:Name="rotateY9"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterY="0.6">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="0,0,1" 
                    Angle="0" 
                    x:Name="rotateZ9"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                        </Transform3DGroup>
                    </ModelVisual3D.Transform>
                </mesh:Cone>

                <mesh:Torus x:Name="Torus1"
Position="-0.7,0.6,0" 
 >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
                            <RotateTransform3D CenterX="-0.7" CenterY="0.6">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="1,0,0" 
                    Angle="0" 
