                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                        </Transform3DGroup>
                    </ModelVisual3D.Transform>
                </mesh:Torus>

                <mesh:Sphere x:Name="Sphere1"
 >
                    <ModelVisual3D.Transform>
                        <Transform3DGroup>
                            <RotateTransform3D CenterX="1.2">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="1,0,0" 
                    Angle="0" 
                    x:Name="rotateX11"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="1.2">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
