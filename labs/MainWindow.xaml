                    Angle="0" 
                    x:Name="rotateY11"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <RotateTransform3D CenterX="1.2">
                                <RotateTransform3D.Rotation>
                                    <AxisAngleRotation3D 
                    Axis="0,0,1" 
                    Angle="0" 
                    x:Name="rotateZ11"/>
                                </RotateTransform3D.Rotation>
                            </RotateTransform3D>
                            <TranslateTransform3D x:Name="myTranslateTransform11"
                                                  OffsetX="3" OffsetY="0" OffsetZ="-7" />

                        </Transform3DGroup>
                    </ModelVisual3D.Transform>
                </mesh:Sphere>

                <mesh:Parallelepiped x:Name="Parallelepiped1"
